// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Matches {
  id         Int      @id
  home_team  String
  away_team  String
  home_score Int
  away_score Int
  date       DateTime
  finished   Boolean

  @@map("matches")
}
